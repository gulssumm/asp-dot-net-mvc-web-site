@model IEnumerable<Council>

<h1>Council home page.</h1>
@if(TempData["keyMessage"] != null)
{<h2> @TempData["keyMessage"] </h2>}
<a class="nav-link text-dark" asp-area="" asp-controller="Council" asp-action="Create">Add a student</a>
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="40%">@obj.Name</td>
                <td width="40%">@obj.Department</td>
                <td><a class="nav-link text-dark" asp-area="" asp-controller="Council" asp-action="Edit" asp-route-id="@obj.Id">Edit</a></td>
                <td><a class="nav-link text-dark" asp-area="" asp-controller="Council" asp-action="Delete" asp-route-id="@obj.Id">Delete</a></td>
            </tr>
        }
    </tbody>
</table>
